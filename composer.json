{
    "name":        "josantonius/hook",
    "version":     "1.0.3",
    "type":        "library",
    "description": "Library for handling hooks.",
    "keywords": [
        "Hook",
        "Actions",
        "Action Hooks",
        "HHVM",
        "PHP"
    ],
    "license": "MIT",
    "authors": [
        {
            "name":     "Josantonius",
            "email":    "hello@josantonius.com",
            "homepage": "https://josantonius.com",
            "role":     "Developer"
        }
    ],
    "support": {
        "source": "https://github.com/josantonius/php-hook"
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "stable",
    "require": {
        "php": "^5.6 || ^7.0"
    },
    "autoload": {
        "psr-4": {
            "Josantonius\\Hook\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Josantonius\\Hook\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
